	VALIGNB(shift)
})



EXTINSN(V6_vror, "Vd32=vror(Vu32,Rt32)", ATTRIBS(A_EXTENSION,A_CVI,A_CVI_VP,A_NOTE_PERMUTE_RESOURCE),
"Align Two vectors by Rt32 as control",
{
	fHIDE(int k;)
	for (k=0;k<fVBYTES();k++) {
		VdV.ub[k] = VuV.ub[(k+RtV)&(fVBYTES()-1)];
	}
	})







