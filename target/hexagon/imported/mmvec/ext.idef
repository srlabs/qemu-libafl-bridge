#define PRINT_VU(TYPE, TYPE2, COUNT)\
    int i;  \
    size4u_t vec_len = fVBYTES();\
    fprintf(stdout,"V%2d: ",VuN);  \
    for (i=0;i<vec_len>>COUNT;i++) {         \
        fprintf(stdout,TYPE2 " ", VuV.TYPE[i]); \
    };  \
    fprintf(stdout,"\\n");  \
	fflush(stdout);\

