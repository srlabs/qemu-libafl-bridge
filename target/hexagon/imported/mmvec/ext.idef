EXTINSN(V6_vscattermhwq,  "if (Qs4) vscatter(Rt32,Mu2,Vvv32.w).h=Vw32", ATTRIBS(A_EXTENSION,A_CVI,A_CVI_SCATTER,A_CVI_VA_DV,A_CVI_VM,A_EA_PAGECROSS,A_MEMSIZE_2B,A_CVI_GATHER_ADDR_4B,A_VTCM_ALLBANK_ACCESS,A_MEMLIKE,A_NOTE_ANY2_RESOURCE), "Scatter halfwords conditional",
{
    fHIDE(int i;)
    fHIDE(int j;)
	fHIDE(int element_size = 2;)
    fHIDE(fSCATTER_INIT( RtV, MuV, element_size);)
    fVLASTBYTE(MuV, element_size);
    fVALIGN(RtV, element_size);
    fVFOREACH(32, i) {
        for(j = 0; j < 2; j++) {
